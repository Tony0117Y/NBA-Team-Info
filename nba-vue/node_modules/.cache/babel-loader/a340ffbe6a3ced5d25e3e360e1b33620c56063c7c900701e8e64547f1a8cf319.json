/n-button>\n        </n-card>\n  \n        <n-card v-else>\n          <h2>Loading team details...</h2>\n        </n-card>\n      </n-layout-content>\n    </n-layout>\n  </template>\n  \n  <script>\n  import { ref, onMounted } from \"vue\";\n  import { useRouter } from \"vue-router\";\n  \n  export default {\n    setup() {\n      const router = useRouter();\n      const team = ref(null);\n  \n      // Fetch the team from sessionStorage on mount\n      onMounted(() => {\n        const storedTeam = sessionStorage.getItem(\"selectedTeam\");\n        if (storedTeam) {\n          team.value = JSON.parse(storedTeam);\n        } else {\n          // If no team is found, navigate back to the team list\n          router.push(\"/\");\n        }\n      });\n  \n      const goBack = () => {\n        router.push(\"/\");\n      };\n  \n      return {\n        team,\n        goBack,\n      };\n    },\n  };\n  </script>\n  \n  <style scoped>\n  h1 {\n    margin: 0;\n  }\n  \n  /* Team Image Styling */\n  .team-logo {\n    max-width: 200px;\n    max-height: 200px;\n    object-fit: contain;\n    border: 2px solid #5e5c5c;\n    border-radius: 10px;\n  }\n  \n  /* Button Styles */\n  .button {\n    background: #1D428A;\n    border-color: #C8102E;\n    color: white;\n  }\n  \n  .button:hover {\n    border-color: #1D428A;\n    background: #C8102E;\n  }\n  </style>"],"mappings":";AA+BE,SAASA,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,SAASC,SAAQ,QAAS,YAAY;AAEtC,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIF,SAAS,CAAC,CAAC;IAC1B,MAAMG,IAAG,GAAIL,GAAG,CAAC,IAAI,CAAC;;IAEtB;IACAC,SAAS,CAAC,MAAM;MACd,MAAMK,UAAS,GAAIC,cAAc,CAACC,OAAO,CAAC,cAAc,CAAC;MACzD,IAAIF,UAAU,EAAE;QACdD,IAAI,CAACI,KAAI,GAAIC,IAAI,CAACC,KAAK,CAACL,UAAU,CAAC;MACrC,OAAO;QACL;QACAF,MAAM,CAACQ,IAAI,CAAC,GAAG,CAAC;MAClB;IACF,CAAC,CAAC;IAEF,MAAMC,MAAK,GAAIA,CAAA,KAAM;MACnBT,MAAM,CAACQ,IAAI,CAAC,GAAG,CAAC;IAClB,CAAC;IAED,OAAO;MACLP,IAAI;MACJQ;IACF,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependenci